    "when": "~2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/underscore": "^1.11.15",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
