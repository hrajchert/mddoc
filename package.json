{
  "name": "mddoc",
  "version": "0.0.2",
  "description": "Unopinionated documentation tool",
  "type": "module",
  "main": "./ts-dist/mddoc.js",
  "exports": {
    ".": "./ts-dist/mddoc.js"
  },
  "directories": {
    "doc": "docs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hrajchert/mddoc.git"
  },
